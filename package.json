{
	"author": "Vanguard Development Team",
	"devDependencies": {
		"@biomejs/biome": "2.3.11",
		"@commitlint/cli": "20.3.1",
		"@commitlint/config-conventional": "20.3.1",
		"@commitlint/types": "20.3.1",
		"@types/node": "25.0.8",
		"husky": "9.1.7",
		"lint-staged": "16.2.7",
		"rimraf": "6.1.2",
		"tsc-alias": "1.8.16",
		"tsx": "4.21.0",
		"turbo": "2.7.4",
		"typescript": "5.9.3"
	},
	"engines": {
		"node": ">=22.18.0"
	},
	"license": "MIT",
	"lint-staged": {
