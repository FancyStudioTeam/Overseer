{
  "name": "overseer",
  "version": "0.0.0@beta",
  "scripts": {
    "dev": "tsx watch --ignore ./src/locales src/Index.ts",
    "build": "tsc",
    "start": "node dist/src/Index.js",
    "format": "npx prettier --write **/*.{ts,js,mjs,json,md,mdx}",
    "ci": "biome ci --formatter-enabled=false --linter-enabled=true --organize-imports-enabled=true --config-path=\"./biome.json\" ."
  },
  "author": "LonelyDeath",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@colors/colors": "1.6.0",
    "@discordjs/formatters": "0.4.0",
    "@napi-rs/canvas": "0.1.52",
    "@prisma/client": "5.14.0",
    "@sapphire/discord-utilities": "3.3.0",
    "@sapphire/ratelimits": "2.4.9",
    "@sapphire/result": "2.6.6",
    "@sapphire/snowflake": "3.5.3",
    "@sapphire/time-utilities": "1.7.12",
    "@sapphire/utilities": "3.16.2",
    "@sentry/node": "8.4.0",
    "dotenv": "16.4.5",
    "glob": "10.4.1",
    "humanize-duration": "3.32.1",
    "ms": "2.1.3",
    "oceanic-collectors": "1.0.7",
    "oceanic.js": "github:OceanicJS/Oceanic",
    "pako": "2.1.0",
    "url-regex-safe": "4.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.3",
    "@types/figlet": "1.5.8",
    "@types/humanize-duration": "3.27.4",
    "@types/ms": "0.7.34",
    "@types/node": "20.12.12",
    "@types/url-regex-safe": "1.0.2",
    "figlet": "1.7.0",
    "prettier": "3.2.5",
    "prisma": "5.14.0",
    "ts-node": "10.9.2",
    "tsx": "4.11.0"
  },
  "prisma": {
    "seed": "tsx src/seed.ts"
  },
  "imports": {
    "#index": "./dist/src/Index.js",
    "#types": "./dist/src/Types.js",
    "#classes": "./dist/src/classes/Client.js",
    "#classes/*": "./dist/src/classes/*",
    "#builders": "./dist/src/builders/Index.js",
    "#util": "./dist/src/util/Util.js",
    "#util/*": "./dist/src/util/*",
    "#constants": "./dist/src/Constants.js",
    "#locales": "./dist/src/locales/Index.js",
    "#locales/*": "./dist/src/locales/Miscellaneous/*",
    "#root/*": "./*"
  },
  "packageManager": "pnpm@9.1.2+sha256.19c17528f9ca20bd442e4ca42f00f1b9808a9cb419383cd04ba32ef19322aba7"
}
