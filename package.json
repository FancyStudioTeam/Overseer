{
  "name": "overseer",
  "version": "0.0.0_beta",
  "scripts": {
    "biome:check": "pnpx @biomejs/biome check --write --config-path=\"./biome.json\" .",
    "biome:ci": "pnpx @biomejs/biome ci --config-path=\"./biome.json\" .",
    "build": "tsc",
    "dev": "SET NODE_ENV=\"DEVELOPMENT\" && tsx watch --ignore ./src/locales src/Index.ts",
    "start": "SET NODE_ENV=\"PRODUCTION\" && cd dist && node src/Index.js",
    "prepare": "husky"
  },
  "author": "LonelyDeath",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@colors/colors": "1.6.0",
    "@discordjs/formatters": "0.4.0",
    "@napi-rs/canvas": "0.1.53",
    "@prisma/client": "5.15.0",
    "@prisma/extension-optimize": "0.8.0",
    "@sapphire/discord-utilities": "3.3.0",
    "@sapphire/ratelimits": "2.4.9",
    "@sapphire/result": "2.6.6",
    "@sapphire/snowflake": "3.5.3",
    "@sapphire/time-utilities": "1.7.12",
    "@sapphire/utilities": "3.16.2",
    "@sentry/node": "8.9.2",
    "dotenv": "16.4.5",
    "glob": "10.4.1",
    "oceanic-builders": "github:FancyStudioTeam/OceanicBuilders",
    "oceanic-collectors": "1.0.7",
    "oceanic.js": "1.10.5-dev.9f95b4a",
    "pako": "2.1.0",
    "ts-pattern": "5.2.0",
    "url-regex-safe": "4.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.1",
    "@types/figlet": "1.5.8",
    "@types/node": "20.14.2",
    "@types/url-regex-safe": "1.0.2",
    "figlet": "1.7.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.7",
    "prisma": "5.15.0",
    "tsx": "4.15.4",
    "typescript": "5.4.5"
  },
  "prisma": {
    "seed": "tsx src/seed.ts"
  },
  "imports": {
    "#base": ["./src/classes/Base.js"],
    "#client": ["./src/classes/Client.js"],
    "#constants": ["./src/util/Constants.js"],
    "#index": ["./src/Index.js"],
    "#locales/*": ["./src/locales/Locales/*"],
    "#package": ["./package.json"],
    "#translations": ["./src/locales/Index.js"],
    "#types": ["./src/Types.js"],
    "#util/*": ["./src/util/*"]
  },
  "packageManager": "pnpm@9.3.0+sha256.e1f9e8d1a16607a46dd3c158b5f7a7dc7945501d1c6222d454d63d033d1d918f",
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css,scss,md}": ["pnpm biome:check"]
  }
}
