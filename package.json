{
	"dependencies": {
		"linkcord": "file:linkcord-0.0.0.tgz"
	},
	"description": "",
	"devDependencies": {
		"@biomejs/biome": "2.3.10",
		"@commitlint/cli": "20.2.0",
		"@commitlint/config-conventional": "20.2.0",
		"@commitlint/types": "20.2.0",
		"@types/node": "25.0.3",
		"cross-env": "10.1.0",
		"husky": "9.1.7",
		"lint-staged": "16.2.7",
		"rimraf": "6.1.2",
		"tsc-alias": "1.8.16",
		"tsx": "4.21.0"
	},
	"engines": {
		"node": ">=22.18.0"
	},
	"license": "MIT",
	"lint-staged": {
		"**/*.{js,jsx,ts,tsx,json}": [
			"pnpm biome:write"
		]
	},
	"main": "dist/index.js",
	"name": "overseer",
	"packageManager": "pnpm@10.0.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/FancyStudioTeam/Overseer"
	},
	"scripts": {
		"biome:check": "biome check",
		"biome:ci": "biome ci",
		"biome:write": "biome check --write",
		"build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
		"prebuild": "pnpm rimraf",
		"prepare": "husky",
		"rimraf": "rimraf dist",
		"start:dev": "cross-env NODE_ENV=development tsx src/index.ts",
		"start:pm2": "pm2 start ecosystem.config.cjs",
		"start:prod": "cross-env NODE_ENV=production node dist/index.js"
	},
	"type": "module",
	"version": "0.0.0"
}
