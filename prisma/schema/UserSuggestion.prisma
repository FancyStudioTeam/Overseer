model UserSuggestion {
  createdAt    DateTime              @default(now())
  general      UserSuggestionGeneral
  id           String                @id @default(auto()) @map("_id") @db.ObjectId
  suggestionId String
}

type UserSuggestionGeneral {
  channelId String
  content   String
  guildId   String
  userId    String
}
