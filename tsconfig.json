{
    "$schema": "https://json.schemastore.org/tsconfig",
    "compileOnSave": true,
    "compilerOptions": {
        "outDir": "dist",
        "target": "ESNext",
        "module": "CommonJS",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "allowUnreachableCode": false,
        "allowUnusedLabels": false,
        "exactOptionalPropertyTypes": false,
        "noImplicitOverride": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "allowSyntheticDefaultImports": true,
        "alwaysStrict": true,
        "pretty": true,
        "noUnusedParameters": true,
        "removeComments": false,
        "importHelpers": false,
        "incremental": true,
        "newLine": "lf",
        "noEmitHelpers": false,
        "noFallthroughCasesInSwitch": true,
        "preserveConstEnums": true,
        "useDefineForClassFields": true,
        "isolatedModules": true,
        "noImplicitAny": true,
        "experimentalDecorators": true,
        "paths": {
            "#builders": ["./src/builders/Index.ts"],
            "#client": ["./src/classes/Client.ts"],
            "#constants": ["./src/util/Constants.ts"],
            "#index": ["./src/Index.ts"],
            "#locales": ["./src/locales/Index.ts"],
            "#package": ["./package.json"],
            "#prisma": ["./src/util/Prisma.ts"],
            "#references": ["./src/locales/Miscellaneous/Reference.ts"],
            "#timezones": ["./src/util/Timezones.ts"],
            "#types": ["./src/Types.ts"],
            "#util": ["./src/util/Util.ts"]
        }
    }
}
